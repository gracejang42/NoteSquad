<!DOCTYPE html>
<html lang="en">
  <head>
    <title><%- my_title %></title>
    <link rel='icon' href='../resources/img/admin_transparent_icon.png' type='image/x-icon'/>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap core local -->
    <link rel="stylesheet" type="text/css" href="../resources/css/libraries/bootstrap.min.4.2.1.css">
    <script src="../resources/js/libraries/jquery-3.6.0.min.js" type="text/javascript"></script>
    <script src="../resources/js/libraries/bootstrap.bundle.min.4.2.1.js" type="text/javascript"></script>

     <!-- custom-->
     <link rel="stylesheet" type="text/css" href="../resources/css/custom/my_style.css">
    <% if(typeof local_css == 'string' && local_css == 'signin.css'){ %>
      <link href="../resources/css/custom/<%=local_css%>" rel="stylesheet">
    <%} else if(typeof my_title == 'string' && my_title == 'Registration Page'){ %>
      <link rel="stylesheet" type="text/css" href="../resources/css/custom/registration.css">
      <script src="../resources/js/custom/registration.js" type="text/javascript"></script>
    <% }else{%>
    <!-- Bootstrap tour -->
    <link rel="stylesheet" type="text/css" href="../resources/css/libraries/bootstrap-tour-standalone.min.css">
    <% } %>

    <% if (typeof error !== 'undefined' && error === true) { %>
        <div class="alert alert-danger"><%= error.message || 'There was a server problem' %></div>
    <% } %>

  </head>
  <body id="customBG">
